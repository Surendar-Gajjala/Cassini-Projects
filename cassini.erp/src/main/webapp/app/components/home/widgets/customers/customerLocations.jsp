<div class="panel panel-default panel-alt widget-messaging" style="min-height: 400px">
    <div class="panel-heading" style="background-color: #E4E7EA; padding:0; height: 56px">
        <div class="row">
            <div class="panel-title col-sm-12" style="font-size:15px; padding: 20px 0 0 20px">Customer Locations</div>
        </div>
    </div>
    <div class="panel-body">
        <ui-gmap-google-map
            center="map.center"
            zoom="map.zoom"
            dragging="map.dragging"
            bounds="map.bounds"
            events="map.events"
            options="map.options"
            pan="true"
            control="map.control">
    </div><!-- panel-body -->
</div><!-- panel -->